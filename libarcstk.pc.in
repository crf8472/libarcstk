prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@CMAKE_INSTALL_PREFIX@
libdir=@INSTALL_LIBRARY_DIR@
sharedlibdir=@INSTALL_LIBRARY_DIR@
includedir=@INSTALL_INCLUDE_DIR@

Name: @PROJECT_NAME@
